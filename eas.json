{
  "cli": {
    "version": ">= 16.13.0",
    "appVersionSource": "remote"
  },

  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": { "simulator": false }
    },

    "development-simulator": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": { "simulator": true },
      "environment": "development"
    },

    "preview": {
      "distribution": "store"
    },

    "production": {
      "autoIncrement": true
    }
  },

  "submit": {
    "production": {}
  }
}
